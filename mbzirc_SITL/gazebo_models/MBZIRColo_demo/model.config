  <depend>
    <model>
      <uri>model://gimbal_small_2d</uri>
      <version>1.0</version>
    </model>
    <model>
      <uri>model://MBZIRColo_base</uri>
      <version>1.0</version>
    </model>
<!--    <model>
      <uri>model://iris_with_standoffs</uri>
      <version>1.0</version>
    </model>-->
    <model>
      <uri>model://MBZIRColo_front_camera</uri>
      <version>1.0</version>
    </model>
    <model>
      <uri>model://MBZIRColo_fake_camera</uri>
      <version>1.0</version>
    </model>
  </depend>
</model>
