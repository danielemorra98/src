<?xml version="1.0"?>

<model>
  <name>MBZIRColo camera RTF</name>
  <version>1.0</version>
  <sdf version="1.6">model.sdf</sdf>

  <maintainer email="a@b.c">zio pino</maintainer>


  <description>
    starting with iris_with_standoffs (che sarebbe MBZIRColo_base)
    add LiftDragPlugin
    add ArduCopterPlugin
    attach gimbal_small_2d model with GimbalSmall2dPlugin
